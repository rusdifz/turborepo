{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@repo/interfaces/*": ["packages/interfaces/src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "apps/frontend-repo/src/app/dashboard/notpage"
  ],
  "exclude": ["node_modules"]
}
